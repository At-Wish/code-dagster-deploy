# Basic Environment Configuration for Dagster
# Copy this file to .env and update the values

# Database Configuration
POSTGRES_USER=postgres_user
POSTGRES_PASSWORD=postgres_password
POSTGRES_DB=postgres_db
POSTGRES_HOST=docker_example_postgresql
POSTGRES_PORT=5432

# Dagster Database Configuration
DAGSTER_POSTGRES_USER=postgres_user
DAGSTER_POSTGRES_PASSWORD=postgres_password
DAGSTER_POSTGRES_DB=postgres_db
DAGSTER_POSTGRES_HOST=docker_example_postgresql
DAGSTER_POSTGRES_PORT=5432

# Dagster Configuration
DAGSTER_HOME=/opt/dagster/dagster_home
DAGSTER_CURRENT_IMAGE=dagster-daemon:latest

# Docker Configuration
DOCKER_NETWORK_NAME=docker_example_network
DOCKER_WEBSERVER_PORT=3000
DOCKER_WEBSERVER_HOST=0.0.0.0

# Run Coordinator Configuration
MAX_CONCURRENT_RUNS=5
CONCURRENCY_LIMIT_KEY=operation
CONCURRENCY_LIMIT_VALUE=example
CONCURRENCY_LIMIT=5

# Storage Configuration
IO_MANAGER_STORAGE_PATH=/tmp/io_manager_storage

# Security Settings
DAGSTER_TELEMETRY_ENABLED=false
